{
  "name": "api-testing-pactumjs",
  "version": "0.0.3",
  "license": "MIT",
  "scripts": {
    "tests:all": "mocha ./tests/**/*.test.js --config configs/mochawesome.config.json",
    "tests:all:health": "mocha ./tests/**/*.test.js --grep @.*-health --config configs/mochawesome.config.json",
    "tests:all:rule": "mocha ./tests/**/*.test.js --grep @.*-regra --config configs/mochawesome.config.json",
    "tests:all:exceptions": "mocha ./tests/**/*.test.js --grep @.*-excecoes --config configs/mochawesome.config.json",
    "tests:login:health": "mocha ./tests/**/*.test.js --grep @Login-health --config configs/mochawesome.config.json",
    "tests:login:rule": "mocha ./tests/**/*.test.js --grep @Login-regra --config configs/mochawesome.config.json",
    "tests:login:exceptions": "mocha ./tests/**/*.test.js --grep @Login-excecoes --config configs/mochawesome.config.json",
    "tests:users:health": "mocha ./tests/**/*.test.js --grep @Usuarios-health --config configs/mochawesome.config.json",
    "tests:users:rule": "mocha ./tests/**/*.test.js --grep @Usuarios-regra --config configs/mochawesome.config.json",
    "tests:users:exceptions": "mocha ./tests/**/*.test.js --grep @Usuarios-excecoes --config configs/mochawesome.config.json",
    "tests:products:health": "mocha ./tests/**/*.test.js --grep @Produtos-health --config configs/mochawesome.config.json",
    "tests:products:rule": "mocha ./tests/**/*.test.js --grep @Produtos-regra --config configs/mochawesome.config.json",
    "tests:products:exceptions": "mocha ./tests/**/*.test.js --grep @Produtos-excecoes --config configs/mochawesome.config.json"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.5.0",
    "dotenv": "^16.4.7",
    "mocha": "^11.1.0",
    "mochawesome": "^7.1.3",
    "pactum": "^3.7.6"
  }
}