{
  "name": "api-testing-pactumjs",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "tests:all": "mocha ./tests/**/*.test.js --reporter mochawesome --reporter-options reportDir=reports,reportFilename=report",
    "tests:all:health": "mocha ./tests/**/*.test.js --grep @.*-health",
    "tests:all:rule": "mocha ./tests/**/*.test.js --grep @.*-regra",
    "tests:all:exceptions": "mocha ./tests/**/*.test.js --grep @.*-excecoes",
    "tests:login:health": "mocha ./tests/**/*.test.js --grep @Login-health",
    "tests:login:rule": "mocha ./tests/**/*.test.js --grep @Login-regra",
    "tests:login:exceptions": "mocha ./tests/**/*.test.js --grep @Login-excecoes",
    "tests:users:health": "mocha ./tests/**/*.test.js --grep @Usuarios-health",
    "tests:users:rule": "mocha ./tests/**/*.test.js --grep @Usuarios-regra",
    "tests:users:exceptions": "mocha ./tests/**/*.test.js --grep @Usuarios-excecoes",
    "tests:products:health": "mocha ./tests/**/*.test.js --grep @Produtos-health",
    "tests:products:rule": "mocha ./tests/**/*.test.js --grep @Produtos-regra",
    "tests:products:exceptions": "mocha ./tests/**/*.test.js --grep @Produtos-excecoes"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.5.0",
    "dotenv": "^16.4.7",
    "mocha": "^11.1.0",
    "mochawesome": "^7.1.3",
    "pactum": "^3.7.6"
  }
}
